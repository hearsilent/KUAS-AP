<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners android:bottomLeftRadius="5dp" android:topLeftRadius="5dp"/>
            <stroke android:width="1dp" android:color="@color/actionbar_color2" />
        </shape>
    </item>
    <item android:bottom="1dp" android:left="1dp" android:top="1dp">
        <shape>
            <padding android:left="5dp"
                android:top="3dp"
                android:right="5dp"
                android:bottom="3dp" />
            <corners android:bottomLeftRadius="4dp" android:topLeftRadius="4dp"/>
            <solid android:color="@color/blue_3" />
        </shape>
    </item>
</layer-list>
